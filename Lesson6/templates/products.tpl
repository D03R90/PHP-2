  <script>
    "use strict";

    $(document).ready(() => {

      let cart = new BigCart('/JS/cart.json');

      let productItem = new Product('./products/?ajax', {
        "numberOfElements": 6,
        "photosId": [127, 128, 129, 130, 131, 132]
      }, cart);

      $('.container_check_item').change(() => {
        let productsOnPage = +$('#show_number').val();
        let checkedItems = $('.container_check_item >input:checked');
        $(productItem.container).children().remove();
        if (checkedItems.length > 0) {
          for (let key of checkedItems) {
            let sizeSort = $(key).attr('name');


            let foundProduct = productItem.products.filter((prod) => (prod.size === sizeSort && prod.sex ===
              "m"));
            for (let i = 0; i < foundProduct.length; i++) {
              if ($('.featured_item').length < productsOnPage) {
                productItem._renderProduct(foundProduct[i]);
              } else {
                return
              };
            }
          }
        };

      });
    });
  </script>


